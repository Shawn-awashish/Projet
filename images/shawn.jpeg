type: '',
duration: 0,
showClose: true,
offset: 60,
position: 'bottom-right',
onClose: function () {
SB.post('/marketnotice/close', {id: item.id}, function (response) {});
}
});
notifyVM.push(vm);
}, 1000 * index)
})
}
})
}
}
);
</script>
